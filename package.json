{
  "name": "dune",
  "description": "web for dune and api",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack server --open --mode development",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "format": "prettier --write '{*.jsx,src/**/*.{js,jsx}}'",
    "lint": "eslint src/ --fix",
    "deploy": "gh-pages -d dist"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run format",
    "post-commit": "git status",
    "pre-push": "git status",
    "preserveUnused": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JapCode/Dune.git"
  },
  "keywords": [
    "dune",
    "api"
  ],
  "author": "japCode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JapCode/Dune/issues"
  },
  "homepage": "https://github.com/JapCode/Dune",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-slick": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@types/regenerator-runtime": "^0.13.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
